Select
SB2.B2_COD AS PRODUTO,
SB1.B1_DESC AS PRODUTO_DESCRICAO,
BM.BM_DESC AS GRUPO_DESCRICACAO,
SB2.B2_LOCAL AS ARMAZEM,
SB2.B2_FILIAL AS FILIAL,
SB2.B2_QATU AS SALDO_ATUAL,
SB2.B2_VATU1 AS SALDO_ATUAL_VALOR,
SB2.B2_CM1 AS CUSTO_UNI

FROM PROTHEUS.SB2010 SB2
LEFT JOIN PROTHEUS.SB1010 SB1 ON SB2.B2_COD  = SB1.B1_COD 
LEFT JOIN PROTHEUS.SBM010 BM ON BM.BM_GRUPO = SB1.B1_GRUPO 

where SB2.B2_FILIAL = '010001' AND SB2.D_E_L_E_T_ <>'*'
